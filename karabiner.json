{
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Ctrl+Space to Ctrl+n to overcome tmux next-window issue",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "spacebar",
                                    "modifiers": { "mandatory": ["left_control"] }
                                },
                                "to": [
                                    {
                                        "key_code": "n",
                                        "modifiers": ["left_control"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Eject to Lock Screen (macOS 10.13+)",
                        "manipulators": [
                            {
                                "from": { "consumer_key_code": "eject" },
                                "to": [
                                    {
                                        "key_code": "q",
                                        "modifiers": ["left_control", "left_command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "CapsLock as ESC, without delay",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "escape",
                                        "lazy": false
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "name": "Default profile",
            "selected": true,
            "virtual_hid_keyboard": { "keyboard_type_v2": "ansi" }
        }
    ]
}
